<?xml version="1.0" encoding="utf-8" ?>
<functions>
  <function>
    <name>empty</name>
    <description>Sets 1 to 0.</description>
    <code>
      -:1:1,0;
    </code>
  </function>
  <function>
    <name>move</name>
    <description>Moves 1 to 2.</description>
    <code>
      -:1:2,0;
      +:2:1;
    </code>
  </function>
  <function>
    <name>copy</name>
    <description>Copies the value from 1 to 2 without changing 1. Uses 3 as auxilliary.</description>
    <code>
      -:1:2,4;
      +:2:3;
      +:3:1;
      -:3:5,0;
      +:1:4;
    </code>
  </function>
  <function>
    <name>s</name>
    <description>Puts [1]+[2]+1 into 2. Uses 3 as auxilliary.</description>
    <code>
      copy:1,2,3:2;
      +:2:0;
    </code>
  </function>
  <function>
    <name>add</name>
    <description>Adds [2] to 1. Uses 3 as auxilliary.</description>
    <code>
      -:2:2,4;
      +:1:3;
      +:3:1;
      -:3:5,0;
      +:2:4;
    </code>
  </function>
  <function>
    <name>sum</name>
    <description>Puts the sum of [1] and [2] into 3. Uses 4 as auxilliary.</description>
    <code>
      copy:1,3,4:2;
      copy:2,3,4:0;
    </code>
  </function>
  <function>
    <name>*</name>
    <description>Multiplies 1 by [2]. Uses 3, 4, and 5 as auxilliaries.</description>
    <code>
      copy:2,3,5:2;
      move:1,4:3;
      -:3:4,5;
      add:1,4,5:3;
      empty:4:0;
    </code>
  </function>
  <function>
    <name>product</name>
    <description>Puts the product of [1] and [2] into 3. Uses 4 and 5 as auxilliaries.</description>
    <code>
      copy:1,4,5:2; <!--n,m,0,n,0-->
      -:4:3,0;
      copy:2,3,5:2;
    </code>
  </function>
  <function>
    <name>^</name>
    <description>Puts [1]^[2] into 1.</description>
    <code>
      copy:1,3,5:2;
      copy:2,4,5:3; <!--n,m,n,m,0-->
      -:4:4,5;
      *:1,3,5,6,7:3;
      empty:3:0;
    </code>
  </function>
  <function>
    <name>power</name>
    <description>Puts [1]^[2] into 3. Uses 4-7 as auxilliary.</description>
    <code>
      copy:1,3,5:2;
      copy:2,4,5:3; <!--n,m,n,m,0-->
      -:4:4,0;
      *:3,1,5,6,7:3;
    </code>
  </function>
  <function>
    <name>difference</name>
    <description>Puts [1]-[2] into 3 if [1]>[2] or 0 otherwise.</description>
    <code>
      copy:1,3,4:2;
      copy:2,4,5:3;
      -:4:4,0;
      -:3:3,3;
    </code>
  </function>
  <function>
    <name>sg</name>
    <description>0 -> 2 if [1] = 0 and 1 -> 2 otherwise.</description>
    <code>
      -:1:2,0;
      +:2:3;
      +:1:0;
    </code>
  </function>
  <!--<function>
    <name>lessThan</name>
    <description>1 -> 3 if [1] less than [2]. 0 -> 3 otherwise.</description>
  </function>-->
  <function>
    <name>%</name>
    <code>
      -:2:2,0;
      +:2:3;
      copy:2,4,5:4;
      copy:1,5,6:5;
      -:4:6,4;
      -:5:5,7;
      copy:2,3,5:8;
      -:4:9,10;
      -:3:8,-1;
      -:3:0,-1;
    </code>
  </function>
</functions>